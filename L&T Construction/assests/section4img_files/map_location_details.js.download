createThumbnail = (mapId, details) => {

$('.map-thumbnail-details').empty();
$('.map-thumbnail-details').append('<div class="thumbnail-wrapper mapContainerOuter"></div>');
let html;
if(details.thumbnail_details.length){	
$('.map-thumbnail-details').append('<button onclick="resetToDefaultMap(); "type="button" class="close thumbnail-close" >×</button>');
$('.thumbnail-wrapper').append('<h6 class="thumbnail-slider-heading"><span>'+details.name+'  ('+details.thumbnail_details.length+' Project)</span></h6>');
	html = `<div class="row-thumbnail owl-carousel owl-theme">`;
	details.thumbnail_details.forEach((element, index) => {
	
   html += ` <div class="item">
						<a onclick="showThumbnailPopup('${index}','${mapId}')"; href="javascript:void(0);" class="thumbnail">
							<img src="${element.thumbnail_image}" alt="Image" style="max-width:100%;" />
						</a>
					  </div>
                `
});
html += `</div>`;
$('.thumbnail-wrapper').append(html);
	
	 var owl = $('.owl-carousel');
              owl.owlCarousel({
               
                nav: true,
                loop: false,
				dots:false,
                responsive: {
                  0: {
                    items: 3
                  },
                  600: {
                    items: 3
                  },
                  1000: {
                    items: 9
                  }
                }
              })
	
	
}
}

showThumbnailPopup = (index,mapId) =>{

	if($("#thumbnailpopupId").length){
		$("#thumbnailpopupId").remove();
	}
	let item =  location_details[mapId].thumbnail_details[index]
let html = `
  <div class="modal  fade" id="thumbnailpopupId" role="dialog">
    <div class="modal-dialog  modal-md">
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
			<img style="width:100%;" src="${item.thumbnail_image}" class="img-responsive">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
			<div class="title">
				<h2>${item.title}</h2>
			</div>
			<div class="thumbnail-description">
				${item.description}
			</div>
        </div>
      </div>
    </div>
  </div>`;
	$(document.body).append(html);
	 $("#thumbnailpopupId").modal("show");
}

resetToDefaultMap = () =>{
  
	$('.map-thumbnail-details').empty();
	$(".mapcontainer .map svg").css("width", "100%");
	$(".mapcontainer .map svg").css("float", "none");
	$maparea2 = $(".mapcontainer");
	$maparea2.trigger('zoom', {level: 0,animDuration: 2000,animEasing:"linear"});
	modal.style.display = "none";
	$maparea2.trigger('update', [{mapOptions: {plots: plots}}]);
	$maparea2.trigger('update', [{mapOptions: {areas: areas_data}}]);
	$(".accordion .card ul").css("display","none");
	$('.mapTooltip').html('');
}

function createProjectsPopup(event, id, mapElem){
	$('.project-popup').remove();
	if(location_details[id]){
	if( location_details[id].thumbnail_details.length)  {
	 mapElem.originalAttrs.src = 'http://optimatest.co.in/media/3s4p5ora/red.png';
						var mouseX = event.pageX;
						var mouseY = event.pageY;	

						var offsetLeft = 10;
						var offsetTop = 20;	

						
					var $map = $(".mapcontainer")			
					
					
					
					var html = '<div class="project-popup">'
						html +=  '<div class="title-block">'
						html +=  '<h5>'+location_details[id].name+'</h5>';
						html +=  '<button onclick="closeProjectPopup()" class="pro-close-bth">x</button>';
						html +=  '</div>'
						html +=  '<ul>'
					for( var i = 0; i < location_details[id].thumbnail_details.length; i++){
						
						html +=  '<li><p><strong>'+location_details[id].thumbnail_details[i].title+' </strong>'+ location_details[id].thumbnail_details[i].description+'</p></li>';
					}
					html +=  '</ul>'
					html +=  '</div>';
					$map.append(html);
					
					var project_popup = $('.project-popup');
					
					var popupPosition = {
					"left": Math.min($map.width() - project_popup.outerWidth() - 5,
					mouseX - $map.offset().left + offsetLeft),
					"top": Math.min($map.height() - project_popup.outerHeight() - 5,
					mouseY - $map.offset().top + offsetTop)
					};	
					
					project_popup.css({
						position:'absolute',
						left:popupPosition.left+'px',
						top:popupPosition.top+'px',
						'max-height': jQuery('.mapcontainer').height() - 80 +'px'
					})
					
					
				}
			}
	}
	
var resizeId;
window.addEventListener('resize', function() {
    clearTimeout(resizeId);
    resizeId = setTimeout(closeProjectPopup, 500);
});



	function closeProjectPopup() {
	$('.project-popup').remove();
		$maparea2 = $(".mapcontainer");
	$maparea2.trigger('zoom', {level: 0,animDuration: 2000,animEasing:"linear"});
	
	$maparea2.trigger('update', [{mapOptions: {plots: plots}}]);
	$maparea2.trigger('update', [{mapOptions: {areas: areas_data}}]);
	}